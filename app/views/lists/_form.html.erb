<%= form_for(@list) do |f| %>
  <% if @list.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@list.errors.count, "error") %> prohibited this list from being saved:</h2>

      <ul>
      <% @list.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class='field'>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>

  <div class='field'>
    <%= f.label :user_id, 'Owner' %><br />
    <%= f.collection_select :user_id, User.all, :id, :name, { include_blank: 'Select Owner/Not Listed' } %>
  </div>

  <div class='actions'>
    <%= f.submit class: 'btn' %>
  </div>
<% end %>
